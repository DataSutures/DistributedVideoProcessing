{
  "pipeline": {
    "name": "colorize"
  },
  "transform": {
    "cmd": [ "python", "./colorize.py", "--prototxt", "../models/colorization_deploy_v2.prototxt", "--caffemodel", "../models/colorization_release_v2.caffemodel"  ],
    "image": "datasutures/caffe_colorization:v5",
    "image_pull_secrets": [ "myregistrykey" ]
   },
  "input": {
    "atom": {
      "glob": "/*",
      "repo": "frames"
    }
  },
  "parallelism_spec": {
    "constant": 2
  },
  "resource_requests": {
   "memory": "1.5G",
   "cpu": 0.5
  },
  "resource_limits": {
    "memory": "2G",
    "cpu": 0.5
  }
}